   z += sign(zsp);
  }
  else{
   break;
  }
 }
    zsp = 0;
}

//z Floor

if (z+zsp &gt; floorZ) {
      zsp = 0
      z = floorZ
      jumps = 1
}

//Movement

x += hsp;
z += zsp;

if (keyboard_check(vk_left) or gamepad_button_check(0,gp_padl)){
