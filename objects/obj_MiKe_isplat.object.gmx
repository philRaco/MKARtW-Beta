down = (keyboard_check_pressed(vk_down) or gamepad_button_check_pressed(0,gp_padd));

hsp = hor*moveSpeed;

//Jumping

if jumps &gt;= 1{ 
        if(keyboard_check(ord("X")) or gamepad_button_check(0,gp_face1)){
             zsp = -jumpSpeed
             jumps = 0;
